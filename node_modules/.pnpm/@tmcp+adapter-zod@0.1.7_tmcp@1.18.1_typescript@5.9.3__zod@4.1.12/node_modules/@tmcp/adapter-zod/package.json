{
  "name": "@tmcp/adapter-zod",
  "version": "0.1.7",
  "description": "Zod adapter for TMCP JSON Schema conversion",
  "type": "module",
  "main": "src/index.js",
  "types": "src/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/types/index.d.ts",
      "default": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "zod": "^4.0.0",
    "tmcp": "^1.16.1"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "@types/json-schema": "^7.0.15",
    "zod": "^4.0.5"
  },
  "keywords": [
    "tmcp",
    "zod",
    "jsonschema",
    "adapter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paoloricciuti/tmcp.git",
    "directory": "packages/adapter-zod"
  },
  "devDependencies": {
    "dts-buddy": "^0.6.2",
    "publint": "^0.3.12",
    "tmcp": "^1.16.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate:types": "dts-buddy && publint",
    "prepublish": "pnpm generate:types"
  }
}